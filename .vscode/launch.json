    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "AllSheets",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/QSArchive.py",
            "args": [
                "All",
                "-vvv",
                "--sheets",
                "All"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "AllEvents",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/QSArchive.py",
            "args": [
                "All",
                "-vvv"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "ParseCSV",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/QSArchive.py",
            "args": [
                "ParseCSV,Render,Prototype",
                "-vvv"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "ParseCSVOnly",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/QSArchive.py",
            "args": [
                "ParseCSV",
                "-vvv"            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Prototype",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/QSArchive.py",
            "args": [
                "Prototype"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "BuildAll",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/QSArchive.py",
            "args": [
                "All",
                "--args",
                "python/config/BuildAll.args"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Testing",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/QSArchive.py",
            "args": [
                "TagMp3"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}